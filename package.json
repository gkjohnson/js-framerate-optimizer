  "author": "Garrett Johnson <garrett.kjohnson@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gkjohnson/js-framerate-optimizer/issues"
  },
  "homepage": "https://github.com/gkjohnson/js-framerate-optimizer#readme",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "babel-jest": "^25.4.0",
    "eslint": "^5.15.3",
    "jest": "^25.4.0",
    "jest-cli": "^25.4.0",
    "opn-cli": "^3.1.0",
    "rollup": "^0.66.6"
